-- Capitalizing first letter of first and last name

UPDATE p2.customers SET first_name = INITCAP(first_name);
UPDATE p2.customers SET last_name = INITCAP(last_name);

-- Capitalizing product and beverage names

UPDATE p2.catalog SET prod_name = INITCAP(prod_name);
UPDATE p2.beverages SET bev_name = INITCAP(bev_name);
